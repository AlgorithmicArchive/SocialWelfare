@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewData["Title"] = "Inidividual";
}

<div class="container-fluid center-align flex-column p-5" style="max-height: 100vh;overflow-y: auto;">
    <input class="form-control width-50 d-flex mx-auto" type="text" name="applicationId" id="applicationId"
        placeholder="Application Id">

    <button class="btn btn-dark d-flex mx-auto mt-2" id="getFlow">Get Application Flow</button>

    <div class="container custom-card p-5 gap-2 mt-2" id="displayList" style="overflow: hidden;display: none;">

    </div>
    <div id="buttons" class="flex gap-3 justify-content-center mt-5" style="display: none;">
        <button class="btn btn-dark" id="previous">Previous</button>
        <button class="btn btn-dark" id="next">Next</button>
    </div>

    <div class="custom-card mt-5 flex-column fs-3" id="displayDetails" style="display: none;">

    </div>
</div>


<script src="/js/Admin/individual.js"></script>